generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Expense {
  id        BigInt      @id @default(autoincrement())
  date      DateTime
  type      String
  category  String
  amount    BigInt
  memo      String?
}

model  Budgets{
  id        BigInt      @id @default(autoincrement())
  Year      Int
  month      Int
  category  String
  amount    BigInt
}